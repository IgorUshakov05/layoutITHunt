<script>
const inputs = document.querySelectorAll('.inputForLabel');

inputs.forEach(input => {
  input.addEventListener('change', () => {
    const parent = input.parentElement;
    
    // Удаляем класс active со всех родителей
    inputs.forEach(input => {
      input.parentElement.classList.remove('active');
    });
    
    // Добавляем класс active к родителю активного элемента input
    parent.classList.add('active');
  });
});

</script>
