                  <img
                    class="selectImg"
                    height="40"
                    width="40"
                    src=""
                    alt="Иконка специальности"
                  />
                  <p class="titleJobItem"></p>
                </div>
                <div class="rightSelection">
                  <span class="canesSelect">Отменить</span>
                </div>
              </div>
              <div class="noneMake">
                <input
                  class="specialisationSelectInput"
                  placeholder="FullStack"
                  type="text"
                  name="specialisationSelect"
                  id="specialisationSelect"
                />
                <div class="ParentlistSpecial">
                  <ul class="listSpecialMy">
                    <li
                      onclick="select('/assets/pictures/analytics.webp','Аналитик')"
